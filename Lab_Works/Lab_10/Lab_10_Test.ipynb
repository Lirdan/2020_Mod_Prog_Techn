    "import numpy as np\n",
    "A = np.random.randint(0,5,(15,3))\n",
    "print(A,\"\\n\")\n",
    "B = np.random.randint(0,5,(2,2))\n",
    "print(B,\"\\n\")\n",
    "C = (A[..., np.newaxis, np.newaxis] == B)\n",
    "rows = (C.sum(axis=(1,2,3)) >= B.shape[1]).nonzero()[0]\n",
    "print(rows)"
